{
    "name": "pugkong/doctrine-entity-factory",
    "description": "Simple Doctrine entity factory for tests",
    "type": "library",
    "homepage": "https://github.com/PugKong/doctrine-entity-factory",
    "license": "WTFPL",
    "autoload": {
        "psr-4": {
            "PugKong\\Doctrine\\EntityFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PugKong\\Doctrine\\EntityFactory\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pug Kong",
            "email": "pugkong@tuta.io"
        }
    ],
    "require": {
        "doctrine/orm": "^2.4",
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "friendsofphp/php-cs-fixer": "^3.17",
        "vimeo/psalm": "^5.12",
        "psalm/plugin-phpunit": "^0.18.4"
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
